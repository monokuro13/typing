<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteEngine on
    RewriteCond %{ HTTPS} off
    RewriteRule ^(.*)$ https://%{ HTTP_HOST}%{REQUEST_URI} [R=301,L]
    RewriteEngine on
    RewriteCond %{HTTP_HOST} ^korean-culture\.net$
    RewriteRule ^(.*)$ https://www.korean-culture.net/$1 [R=301,L]
</IfModule>
